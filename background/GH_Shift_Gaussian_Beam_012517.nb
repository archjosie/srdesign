Cell["\<\
These formula can be used to calculate the GH shifts for each polarization \
using Artmann\[CloseCurlyQuote]s formulae:\
\>", "Text",
 CellChangeTimes->{{3.6786286600492563`*^9, 3.678628676017026*^9}, {
  3.67873873811117*^9, 3.6787387435450907`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"GHshift$1", ",", "GHshift$2"}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"GHshift$1", "[", 
    RowBox[{"\[Theta]_", ",", "n_"}], "]"}], "=", 
   RowBox[{
    FractionBox["1", "k0"], 
    RowBox[{"FullSimplify", "[", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"\[CapitalPhi]1$analytic", "[", 
         RowBox[{"\[Theta]", ",", "n"}], "]"}], ",", "\[Theta]"}], "]"}], ",", 
      RowBox[{
       RowBox[{"1", "-", 
        SuperscriptBox["n", "2"], "-", 
        SuperscriptBox[
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "2"]}], ">", "0"}]}], 
     "]"}]}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GHshift$2", "[", 
   RowBox[{"\[Theta]_", ",", "n_"}], "]"}], "=", 
  RowBox[{
   FractionBox["1", "k0"], 
   RowBox[{"FullSimplify", "[", 
    RowBox[{
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"\[CapitalPhi]2$analytic", "[", 
        RowBox[{"\[Theta]", ",", "n"}], "]"}], ",", "\[Theta]"}], "]"}], ",", 
     
     RowBox[{
      RowBox[{"1", "-", 
       SuperscriptBox["n", "2"], "-", 
       SuperscriptBox[
        RowBox[{"Cos", "[", "\[Theta]", "]"}], "2"]}], ">", "0"}]}], 
    "]"}]}]}]}], "Input",
